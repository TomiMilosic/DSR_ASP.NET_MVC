@model double;


@{

    <label>
        <input type="range" name="@ViewData.ModelMetadata.PropertyName" id="@ViewData.ModelMetadata.PropertyName" min="1" max="100,00" step="0.1"  value="1"
               onchange="$('~ output', this).val(value)"
               oninput="$('~ output', this).val(value)">
        <output>1,0</output>
    </label>

}